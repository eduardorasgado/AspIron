@model List<Alumno>

@{
    ViewData["Title"] = "Todos los alumnos";
    Layout = "SimpleLayout";
}

<div class="text-center">
    <h1>@ViewData["Title"]</h1>
</div>
<div class="jumbotron">
    @foreach (var alumno in @Model)
    {
        <partial name="AcademyBaseSimple" model="alumno"/>
    }
</div>